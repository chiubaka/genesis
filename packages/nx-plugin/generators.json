{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-plugin",
  "version": "0.0.1",
  "generators": {
    "ci": {
      "factory": "./src/generators/workspace/ci",
      "schema": "./src/generators/workspace/ci/schema.json",
      "description": "Generates CI configuration"
    },
    "circleci": {
      "factory": "./src/generators/workspace/ci/circleci",
      "schema": "./src/generators/workspace/ci/circleci/schema.json",
      "description": "Generates CircleCI configuration"
    },
    "codecov": {
      "factory": "./src/generators/workspace/testing/codecov",
      "schema": "./src/generators/workspace/testing/codecov/schema.json",
      "description": "Generates Codecov configuration"
    },
    "eslint": {
      "factory": "./src/generators/workspace/linting/eslint",
      "schema": "./src/generators/workspace/linting/eslint/schema.json",
      "description": "Generates ESLint configuration and setup"
    },
    "git": {
      "factory": "./src/generators/workspace/git",
      "schema": "./src/generators/workspace/git/schema.json",
      "description": "Generates a git repository"
    },
    "gitHooks": {
      "factory": "./src/generators/workspace/git/gitHooks",
      "schema": "./src/generators/workspace/git/gitHooks/schema.json",
      "description": "Generates a git hooks setup"
    },
    "github": {
      "factory": "./src/generators/workspace/git/github",
      "schema": "./src/generators/workspace/git/github/schema.json",
      "description": "Generates a GitHub repo and standardizes settings and labels"
    },
    "github.branch-protection": {
      "factory": "./src/generators/workspace/git/github/branchProtection",
      "schema": "./src/generators/workspace/git/github/branchProtection/schema.json",
      "description": "Configures branch protection for the master branch of a GitHub repo"
    },
    "github.labels": {
      "factory": "./src/generators/workspace/git/github/labels",
      "schema": "./src/generators/workspace/git/github/labels/schema.json",
      "description": "Generates a standardized set of GitHub labels for issues"
    },
    "jest": {
      "factory": "./src/generators/workspace/testing/jest",
      "schema": "./src/generators/workspace/testing/jest/schema.json",
      "description": "Installs Jest and generates a standard config"
    },
    "linting": {
      "factory": "./src/generators/workspace/linting",
      "schema": "./src/generators/workspace/linting/schema.json",
      "description": "Generates linting setup including EsLint and Prettier"
    },
    "lib.node": {
      "factory": "./src/generators/lib/node",
      "schema": "./src/generators/lib/node/schema.json",
      "description": "Generates a shareable node library"
    },
    "lib.node.e2e": {
      "factory": "./src/generators/lib/node/e2e",
      "schema": "./src/generators/lib/node/e2e/schema.json",
      "description": "Generates E2E testing framework for a shareable node library"
    },
    "preset": {
      "factory": "./src/generators/workspace/preset",
      "schema": "./src/generators/workspace/preset/schema.json",
      "description": "Generates a full workspace with batteries included!"
    },
    "prettier": {
      "factory": "./src/generators/workspace/linting/prettier",
      "schema": "./src/generators/workspace/linting/prettier/schema.json",
      "description": "Generates Prettier configuration and setup"
    },
    "project.eslint": {
      "factory": "./src/generators/project/eslint",
      "schema": "./src/generators/project/eslint/schema.json",
      "description": "Generates an ESLint config for a project"
    },
    "project.readme": {
      "factory": "./src/generators/project/readme",
      "schema": "./src/generators/project/readme/schema.json",
      "description": "Generates a README for a project"
    },
    "readme": {
      "factory": "./src/generators/workspace/readme",
      "schema": "./src/generators/workspace/readme/schema.json",
      "description": "Generates a README for the repo"
    },
    "testing": {
      "factory": "./src/generators/workspace/testing",
      "schema": "./src/generators/workspace/testing/schema.json",
      "description": "Generates monorepo-wide testing configuration and setup"
    },
    "tsconfig": {
      "factory": "./src/generators/workspace/tsconfig",
      "schema": "./src/generators/workspace/tsconfig/schema.json",
      "description": "Generates a tsconfig extending from @chiubaka/tsconfig and installs relevant dependencies"
    }
  }
}

{
  "$schema": "http://json-schema.org/schema",
  "$id": "Git",
  "title": "",
  "type": "object",
  "properties": {
    "commitMessage": {
      "type": "string",
      "description": "Initial repository commit message."
    },
    "committerEmail": {
      "type": "string",
      "description": "Initial repository commit author's email address."
    },
    "committerName": {
      "type": "string",
      "description": "Initial repository commit author's name."
    },
    "skipGitHooks": {
      "type": "boolean",
      "description": "Whether or not to skip Git hooks generation"
    },
    "skipGitHub": {
      "type": "boolean",
      "description": "Whether or not to skip GitHub repository generation"
    }
  },
  "required": ["commitMessage"]
}

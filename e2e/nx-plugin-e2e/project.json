{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/nx-plugin-e2e/tests",
  "targets": {
    "e2e": {
      "dependsOn": [{ "target": "build", "projects": "dependencies" }],
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "e2e/nx-plugin-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": [
        "reports/coverage/e2e/nx-plugin-e2e",
        "reports/junit/nx-plugin-e2e.xml"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/nx-plugin-e2e"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["nx-plugin"]
}

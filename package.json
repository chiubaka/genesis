{
  "name": "nx-plugin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nx build",
    "build:affected": "nx affected --target=build",
    "build:ci": "yarn build:affected --base=$NX_BASE --head=$NX_HEAD",
    "deploy": "nx deploy $@ --configuration=production",
    "deploy:ci": "yarn deploy",
    "deploy:genesis:local": "./scripts/deployGenesisLocal.sh",
    "deploy:preset:local": "./scripts/deployPresetLocal.sh",
    "lint": "nx lint",
    "lint:affected": "nx affected --target=lint",
    "lint:all": "yarn lint:root && yarn lint:packages",
    "lint:ci": "yarn lint:root && yarn lint:affected --base=$NX_BASE --head=$NX_HEAD",
    "lint:fix:all": "yarn lint:fix:root; yarn lint:fix:packages",
    "lint:fix:packages": "yarn lint:packages --fix",
    "lint:fix:root": "yarn lint:root --fix",
    "lint:packages": "nx run-many --target=lint --all",
    "lint:root": "yarn eslint .",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "start": "nx serve",
    "start:verdaccio": "docker-compose up -d && npx npm-cli-login -u test -p test -e test@chiubaka.com -r http://localhost:4873",
    "test": "nx test",
    "test:affected": "nx affected --target=test",
    "test:ci": "yarn test:affected --ci --coverage --base=$NX_BASE --head=$NX_HEAD && yarn test:e2e:affected --base=$NX_BASE --head=$NX_HEAD",
    "test:e2e": "nx run-many --target=e2e --all",
    "test:e2e:affected": "nx affected --target=e2e"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@swc/helpers": "~0.3.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@chiubaka/eslint-config": "^0.4.1",
    "@chiubaka/tsconfig": "^0.1.0",
    "@nrwl/cli": "15.0.13",
    "@nrwl/devkit": "15.0.13",
    "@nrwl/eslint-plugin-nx": "15.0.13",
    "@nrwl/jest": "15.0.13",
    "@nrwl/js": "15.0.13",
    "@nrwl/linter": "15.0.13",
    "@nrwl/nx-plugin": "15.0.13",
    "@nrwl/workspace": "15.0.13",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/eslint": "^8.4.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "28.1.8",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "^26.6.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-junit": "^14.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "nx": "15.0.13",
    "portfinder": "^1.0.28",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "verdaccio": "^5.14.0",
    "verdaccio-auth-memory": "^10.2.0",
    "verdaccio-memory": "^10.3.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "packageManager": "yarn@3.2.3"
}

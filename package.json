{
  "name": "nx-plugin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nx build",
    "build:affected": "nx affected --target=build",
    "build:ci": "yarn build:affected --base=$NX_BASE --head=$NX_HEAD",
    "deploy": "nx deploy",
    "deploy:affected": "nx affected --target=deploy",
    "deploy:ci": "yarn deploy:affected --base=$NX_BASE --head=$NX_HEAD",
    "deploy:preset:local": "./scripts/deployPresetLocal.sh",
    "lint": "nx lint",
    "lint:affected": "nx affected --target=lint",
    "lint:all": "yarn lint:root && yarn lint:packages",
    "lint:ci": "yarn lint:root && yarn lint:affected --base=$NX_BASE --head=$NX_HEAD",
    "lint:fix:all": "yarn lint:fix:root; yarn lint:fix:packages",
    "lint:fix:packages": "yarn lint:packages --fix",
    "lint:fix:root": "yarn lint:root --fix",
    "lint:packages": "nx run-many --target=lint --all",
    "lint:root": "yarn eslint .",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "start": "nx serve",
    "test": "nx test",
    "test:affected": "nx affected --target=test",
    "test:ci": "yarn test:affected --ci --coverage --base=$NX_BASE --head=$NX_HEAD && yarn test:e2e:affected --base=$NX_BASE --head=$NX_HEAD",
    "test:e2e": "nx run-many --target=e2e --all",
    "test:e2e:affected": "nx affected --target=e2e"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@swc/helpers": "~0.3.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@chiubaka/eslint-config": "^0.3.7",
    "@nrwl/cli": "14.4.3",
    "@nrwl/devkit": "14.4.3",
    "@nrwl/eslint-plugin-nx": "14.4.3",
    "@nrwl/jest": "14.4.3",
    "@nrwl/js": "14.4.3",
    "@nrwl/linter": "14.4.3",
    "@nrwl/nx-plugin": "^14.4.3",
    "@nrwl/workspace": "14.4.3",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/eslint": "^8.4.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "^26.6.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "nx": "14.4.3",
    "portfinder": "^1.0.28",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2",
    "verdaccio": "^5.14.0",
    "verdaccio-auth-memory": "^10.2.0",
    "verdaccio-memory": "^10.3.0"
  }
}
